{
  "name": "@paradigm_ts/testproject",
  "version": "1.0.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "author": "Paradigm Trading Systems",
  "licence": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rufcoder/TestProject.git"
  },
  "scripts": {
    "start": "dotenv -- nodemon -e ts -x ts-node src/index.ts",
    "debug": "node --inspect-brk=5858 src/collections/RBTree/index2.ts",
    "dev": "npm-run-all --parallel start playground",
    "build": "rimraf build && tsc --keyofStringsOnly",
    "testing": "jest",
    "test": "npm run testing -- --watch",
    "testcover": "jest --coverage --watch",
    "preversion": "npm run test",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags",
    "gendocs": "rimraf docs && node_modules/.bin/typedoc --options ./typedoc.js --excludePrivate",
    "gendevdocs": "rimraf docs-dev && node_modules/.bin/typedoc --options ./typedoc.js --out ./docs-dev"
  },
  "dependencies": {
    "@paradigm_ts/pfmbase": "^1.0.49"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "dotenv-cli": "^1.4.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "jest-extended": "^0.7.2",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/__scratchPad__/index2.ts",
      "!src/index.ts",
      "src/**/*.ts"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "jest-extended",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx)$",
    "coverageDirectory": "src/__coverage__",
    "coveragePathIgnorePatterns": [
      ".d.ts"
    ]
  },
  "description": "> Just testing project initialization settings",
  "bugs": {
    "url": "https://github.com/rufcoder/TestProject/issues"
  },
  "homepage": "https://github.com/rufcoder/TestProject#readme"
}
